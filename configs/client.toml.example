[server]
url="wss://tunnel.example.com/ws"
token="REPLACE_WITH_SERVER_TOKEN"

[reconnect]
initial_delay=1
max_delay=60
multiplier=2

[cloudflare]
enabled=false
ips=[]
host=""
check_interval=300

# [security]
# max_connections=0  # 0 = unlimited, or set a limit
# connection_timeout=300
# allowed_destinations=["0.0.0.0/0"]  # Restrict which IPs client can connect to

[logging]
level="info"
file="/var/log/ghostwire-client.log"
